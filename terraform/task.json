[
    {
        "name": "backend-app", 
        "image": "324139215624.dkr.ecr.us-east-1.amazonaws.com/giveandtake-devl:latest", 
        "portMappings": [
            {
                "containerPort": 443, 
                "hostPort": 443, 
                "protocol": "tcp"
            }
        ], 
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "backend-logs-devl",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "backend"
            }
        },
        "essential": true
    }
] 
